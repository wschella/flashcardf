     ml:Hero_Stories ,
     ml:Centered_Charles_the_Great ,
     ml:Feudal_Spirit ,
     ml:Oral_Hand_Down ,
     ml:Rhyme .

ml:Courtly_Literature a lit:Genre ;
  rdf:label "hoofse literatuur" ;
  lit:inLiteraturePeriod ml:High_Middle_Ages ;
  lit:characterizedBy
    ml:Feudal_Spirit ,
    ml:Christian_Elements ,
    ml:Knightly ,
    ml:Performed_by_Troubadours ,
    ml:Courtly_Love_Ideal .

ml:Courtly_Knight_Novel a lit:Genre ;
  rdf:label "ridderroman" ;
  lit:isSubGenreOf ml:Courtly_Literature ;
  lit:inTimePeriod lit:11th, lit:12th, lit:13th ;
