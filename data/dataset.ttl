@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix lit: <dataset/ontology.ttl#> .
@prefix ml: <dataset/data.ttl#> .

# Ontology --------------------------------------------------------------------

lit:Writer a owl:Class .

lit:Book a owl:Class .

lit:TimePeriod a owl:Class .

lit:LitaturePeriod a owl:Class .

lit:Genre a owl:Class .

lit:Trait a owl:Class .

lit:LiteraryType a owl:Class .

lit:writtenBy a owl:ObjectProperty ;
  rdfs:domain lit:Book ;
  rdfs:range lit:Writer .

lit:hasGenre a owl:ObjectProperty ;
  rdfs:domain lit:Book ;
  rdfs:range lit:Genre .

lit:name a owl:DatatypeProperty ;
  rdfs:domain lit:Writer ;
  rdfs:range xsd:string .

lit:title a owl:DatatypeProperty ;
  rdfs:domain lit:Book ;
  rdfs:range xsd:string .

lit:inLiteraturePeriod a owl:ObjectProperty ;
  rdfs:domain lit:Genre ;
  rdfs:range lit:LitaturePeriod .

lit:inTimePeriod a owl:ObjectProperty ;
  rdfs:domain lit:LitaturePeriod ;
  rdfs:domain lit:Genre ;
  rdfs:range lit:TimePeriod .

lit:writtenIn a owl:ObjectProperty ;
  rdfs:domain lit:Book ;
  rdfs:range lit:TimePeriod .

lit:livedIn a owl:ObjectProperty ;
  rdfs:domain lit:Writer ;
  rdfs:range lit:TimePeriod .

lit:characterizedBy a owl:ObjectProperty ;
  rdfs:domain lit:Genre ;
  rdfs:domain lit:Writer ;
  rdfs:range lit:Trait .

lit:wiki a owl:ObjectProperty .

lit:hasType a owl:ObjectProperty ;
  rdfs:domain lit:Book ;
  rdfs:range lit:LiteraryType .

lit:isSubGenreOf a owl:ObjectProperty ;
  rdfs:domain lit:Genre ;
  rdfs:range lit:Genre .

