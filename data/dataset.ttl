@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix lit: <dataset/ontology.ttl#> .
@prefix ml: <dataset/data.ttl#> .



lit:Writer a owl:Class .

lit:Book a owl:Class .

lit:Period a owl:Class .

lit:Genre a owl:Class .

lit:hasPeriod a owl:ObjectProperty .

lit:writtenBy a owl:ObjectProperty ;
  rdfs:domain lit:Book ;
  rdfs:range lit:Writer .

lit:hasGenre a owl:ObjectProperty ;
  rdfs:domain lit:Book ;
  rdfs:range lit:Genre .

lit:name a owl:DatatypeProperty ;
  rdfs:domain lit:Writer ;
  rdfs:range xsd:string .

lit:title a owl:DatatypeProperty ;
  rdfs:domain lit:Book ;
  rdfs:range xsd:string .

