# Genres ----------------------------------------------------------------------

ml:Old_Germanic_Literature a lit:Genre ;
  rdf:label "Oudgermaanse literatuur"@nl ;
  lit:inLiteraturePeriod ml:Early_Middle_Ages ;
  lit:inTimePeriod
    ml:8th ,
    ml:9th ,
    ml:10th ,
    ml:11th ,
    ml:12th ,
    ml:13th ;
  lit:characterizedBy
    ml:Oral_Hand_Down ,
    ml:Performed_by_Military_Poets ,
    ml:Festivities ,
    ml:Rhyme ,
    ml:Simple ,
    ml:Christian_Elements .

ml:Old_Scandinavian_Literature a lit:Genre ;
  rdf:label "Oudscandinavische literatuur"@nl ;
  lit:inLiteraturePeriod ml:Early_Middle_Ages ;
  lit:isSubGenreOf ml:Old_Germanic_Literature ;
  lit:inTimePeriod ml:8th, ml:9th, ml:10th, ml:11th, ml:12th, ml:13th ;
  lit:characterizedBy
    ml:Oral_Hand_Down ,
    ml:Old_Scandinavian_Literature .

ml:Chanson_de_geste a lit:Genre ;
  rdf:label "Chanson de geste" ;
  lit:inTimePeriod ml:9th ;
