@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ml: <http://example.com/> .


ml:Writer a owl:Class .

ml:Book a owl:Class .

ml:Period a owl:Class .

ml:Genre a owl:Class .

ml:hasPeriod a owl:ObjectProperty .

ml:writtenBy a owl:ObjectProperty ;
  rdfs:domain ml:Book ;
  rdfs:range ml:Writer .

ml:hasGenre a owl:ObjectProperty ;
  rdfs:domain ml:Book ;
  rdfs:range ml:Genre .

ml:name a owl:DatatypeProperty ;
  rdfs:domain ml:Writer ;
  rdfs:range xsd:string .

ml:title a owl:DatatypeProperty ;
  rdfs:domain ml:Book ;
  rdfs:range xsd:string .

# -----------------------------

